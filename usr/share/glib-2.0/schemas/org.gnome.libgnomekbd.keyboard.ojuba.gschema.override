[org.gnome.libgnomekbd.keyboard]
layouts=['us', 'ara']
model=''
options=['altwin:super_win', 'grp_led:scroll', 'grp:alt_shift_toggle', 'terminate:ctrl_alt_bksp']

[org.gnome.libgnomekbd.desktop]
default-group=0
group-per-window=true

[org.gnome.desktop.input-sources]
sources=[('xkb', 'us'), ('xkb', 'ara')]
per-window=true
xkb-options=['altwin:super_win', 'grp_led:scroll', 'grp:alt_shift_toggle', 'terminate:ctrl_alt_bksp']

